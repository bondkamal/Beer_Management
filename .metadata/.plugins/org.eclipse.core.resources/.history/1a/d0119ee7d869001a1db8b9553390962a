package sum_Of;

public class Sum_of_array {
	
	static int arr[] = {20,30,85};
	
	static int sum()
	{
	 
		int sum = 0;
		int i;
		
		for(i = 0; i<arr.length; i++)
				sum += arr[i];
		return sum;
		
		
	}
	
	

	public static void main(String[] args) {
		
		System.out.println("sum of the array is "+ sum());

	}

}
